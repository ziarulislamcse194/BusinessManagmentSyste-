
@model BusinessManagementApp.Models.Models.Supplier

@{
    Layout = null;
}

@if (ViewBag.SuccesMsg != null)
{
    <h3 style="font-family: 'Times New Roman', Times, Serif; color: darkblue"> @ViewBag.SuccesMsg</h3>
}

@if (ViewBag.FailedMsg != null)
{
    <h3 style="font-family: 'Times New Roman', Times, Serif; color: darkblue"> @ViewBag.FailedMsg</h3>
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
    
</head>
<body>
<div>
    <form method="post">
        <table>
            <tr>
                <td>Code</td>
                <td>
                    <input type="text" name="Code" value="@Model.Code">
                </td>
            </tr>

            <tr>
                <td>Name</td>
                <td>
                    <input type="text" name="Name" value="@Model.Name">
                </td>
            </tr>

            <tr>
                <td>Address</td>
                <td>
                    <input type="text" name="Address" value="@Model.Address">
                </td>
            </tr>

            <tr>
                <td>Email</td>
                <td>
                    <input type="text" name="Email" value="@Model.Email">
                </td>
            </tr>

            <tr>
                <td>Contact</td>
                <td>
                    <input type="number" name="Contact" value="@Model.Contact">
                </td>
            </tr>

            <tr>
                <td>ContactPerson</td>
                <td>
                    <input type="text" name="ContactPerson" value="@Model.ContactPerson">
                </td>
            </tr>

            <tr>
                <td>Action</td>
                <td>
                    <input type="submit" value="Edit">
                </td>
            </tr>
        </table>
    </form>
</div>
</body>
</html>
